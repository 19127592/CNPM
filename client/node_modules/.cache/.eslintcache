assword] = useState(\"\");\r\n\r\n  const setUserPassword = (e) => {\r\n    const { name, value } = e.target;\r\n    setUser({ ...user, [name]: value });\r\n  };\r\n  const login = async (e) => {\r\n    e.preventDefault();\r\n    try {\r\n      await axios.post(\"/user/login\", { ...user });\r\n      localStorage.setItem(\"firstTime_Login\", true);\r\n      window.location.href = \"/\";\r\n    } catch (err) {\r\n      alert(err.response.data.msg);\r\n    }\r\n  };\r\n  return (\r\n    <div className=\"login-page\">\r\n      <div className=\"title\">Login</div>\r\n      <div className=\"container\">\r\n        <form onSubmit={login}>\r\n          <div className=\"form-group\">\r\n            <span className=\"form-title\">Email</span>\r\n            <input\r\n              type=\"email\"\r\n              className=\"input-box\"\r\n              name=\"email\"\r\n              required\r\n              placeholder=\"Email address\"\r\n              value={user.email}\r\n              onChange={setUserPassword}\r\n            />\r\n          </div>\r\n          <div className=\"form-group\">\r\n            <span className=\"form-title\">Password</span>\r\n            <input\r\n              type=\"password\"\r\n              className=\"input-box\"\r\n              name=\"password\"\r\n              required\r\n              autoComplete=\"on\"\r\n              placeholder=\"Password\"\r\n              value={user.password}\r\n              onChange={setUserPassword}\r\n            />\r\n          </div>\r\n          <div className=\"row\">\r\n            <button type=\"submit\" className=\"re-btn\">\r\n              Login\r\n            </button>\r\n            <button type=\"submit\" className=\"re-re-btn\">\r\n              <Link to=\"/register\">Register</Link>\r\n            </button>\r\n            <div className=\"passwordforgot\">\r\n              <Link to=\"/forgotpassword\">Forgot password?</Link>\r\n            </div>\r\n          </div>\r\n        </form>\r